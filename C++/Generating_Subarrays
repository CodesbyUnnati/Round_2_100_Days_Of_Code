#include<iostream>
using namespace std;
int main()

{
    int n;
    int a[1000];
    cout<<"Enter the numbers";
    cin>>n;
    cout<<"Enter Array: ";
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
        for(int j=i;j<n;j++)
        {
            for(int k=i;k<=j;k++)
            {
                cout<<a[k]<<" ,";
               
            }
        }
    }
    return 0;
}
