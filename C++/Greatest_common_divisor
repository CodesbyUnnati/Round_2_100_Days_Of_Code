#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cout<<"Enter the numbers : ";
    cin>>a>>b;
    if(a==0)
    {
    return b;
    }
    if(b==0)
    {
    return a;
    }
    while(a!=b)
    {
    if(a>b)
    {
        a-=b;
    }
    else
    {
    b-=a;
    }
    
    }
    cout<<"GCD IS: "<<a;
    
    return 0;
}
